 RowBox[{
  RowBox[{"GeneratePoints", "[", 
   RowBox[{"x_", ",", "y_", ",", "q_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"a", ",", "b", ",", "c", ",", "d"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"a", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"2", "i", "*", "q"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "x"}], "}"}]}], "]"}]}], ";", 
